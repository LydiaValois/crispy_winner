postcards = {
    "Maria": "London",
    "Lorenzo": "Milan",
    "Oleg": "Canberra",
    "Hans": "Calgary",
    "Mark": "Milan",
    "Alex": "Krakow",
    "Julia": "Murmansk"}
new = {"Petra": "Paris", "Ivan": "Moscow"}
postcards.update(new)
postcards['Oleg'] = 'Sydney'
cities = set(postcards.values())
print('В коллекции Алисы', len(cities), 'уникальных городов:', ', '.join(cities), end='.')
